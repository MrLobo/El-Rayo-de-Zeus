import base_material from "Normal_AO_Specular_Mapping_Shader.material"

material cauldron : base_material
{
	set_texture_alias diffuseMap 	texture_cauldron_diffuse.tga
	set_texture_alias specMap 		texture_cauldron_specular.tga
    
    set $diffuseCol  "1 1 1"
    set $specularCol "1 1 1"
    set $shininess "20" 
}


