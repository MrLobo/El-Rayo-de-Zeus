<?xml version="1.0" encoding="UTF-8"?>

<GUILayout >
    <Window Type="DefaultWindow" Name="NetLobbyServer" >
        <Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{1,0}}" />
        <Window Type="OgreTray/StaticImage" Name="NetLobbyServer/BackGround" >
			<Property Name="FrameEnabled" Value="False" />
			<Property Name="BackgroundEnabled" Value="False" />		
			<Property Name="Image" Value="set:BackgroundInit image:BackgroundInit" />			
            <Property Name="AlwaysOnTop" Value="False" />			
            <Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{1,0}}" />
            <Window Type="OgreTray/Title" Name="NetLobbyServer/BackGround/" >
                <Property Name="Font" Value="Diogenes-24" />
                <Property Name="Text" Value="WAITING FOR CLIENTS" />
                <Property Name="AlwaysOnTop" Value="True" />
                <Property Name="UnifiedAreaRect" Value="{{0.3,0},{0.2,0},{0.7,0},{0.3,0}}" />
            </Window>
        </Window>

        <Window Type="OgreTray/Button" Name="NetLobbyServer/Start" >
            <Property Name="Font" Value="DejaVuSans-10" />
            <Property Name="Text" Value="Begin game" />
            <Property Name="AlwaysOnTop" Value="True" />
            <Property Name="UnifiedAreaRect" Value="{{0.80,0},{0.9,0},{0.95,0},{0.95,0}}" />
        </Window>

		 <Window Type="OgreTray/StaticText" Name="NetLobbyServer/Status" >
            <Property Name="Font" Value="DejaVuSans-10" />
            <Property Name="Text" Value="Status: Initializing server ..." />
            <Property Name="AlwaysOnTop" Value="False" />
            <Property Name="UnifiedAreaRect" Value="{{0.1,1},{0.4,0},{0.9,0},{0.6,0}}" />
        </Window>
		
        <Window Type="OgreTray/Button" Name="NetLobbyServer/Back" >
            <Property Name="Font" Value="DejaVuSans-10" />
            <Property Name="Text" Value="Back" />
            <Property Name="AlwaysOnTop" Value="True" />
            <Property Name="UnifiedAreaRect" Value="{{0.05,0},{0.9,0},{0.2,0},{0.95,0}}" />
        </Window>
    </Window>
</GUILayout>
