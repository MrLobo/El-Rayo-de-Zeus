import base_material from "Normal_AO_Specular_Mapping_Shader.material"

material SpartanHelmet : base_material
{
	set_texture_alias diffuseMap 	helmet_DF.tga
    set_texture_alias normalMap 	helmet_NM.tga
    set_texture_alias specMap 		helmet_SPC.tga

	set $diffuseCol "1 1 1"
    set $specularCol "0.816 0.722 0.537"
    set $shininess "30" 
}






