import base_material from "Normal_AO_Specular_Mapping_Shader.material"

material Hades_throne : base_material
{
	set_texture_alias diffuseMap 	texture_hades_throne.tga
	set_texture_alias specMap 		texture_hades_throne_SL.tga
    
    set $diffuseCol  "1 1 1"
    set $specularCol "1 1 1"
    set $shininess "20" 
}