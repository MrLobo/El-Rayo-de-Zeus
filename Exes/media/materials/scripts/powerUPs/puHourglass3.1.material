import base_material from "Normal_AO_Specular_Mapping_Shader.material"

material Hourglass : base_material
{
	set_texture_alias diffuseMap 	Hourglass_diffuse.tga
	set_texture_alias specMap 		Hourglass_SL.tga
    
    set $diffuseCol  "1 1 1"
    set $specularCol "1 1 1"
    set $shininess "20" 
}

